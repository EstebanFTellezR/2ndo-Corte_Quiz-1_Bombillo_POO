public class Principal {

    public static void main(String[] args) {
        Bombillo bombillosala = new Bombillo(true);
        bombillosala.estaPrendido();
        
        bombillosala.apagar();
        bombillosala.estaPrendido();
        
        bombillosala.prender();
        bombillosala.estaPrendido();
        
    }    
}
